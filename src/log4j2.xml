<?xml version="1.0" encoding="UTF-8"?>
<configuration status="WARN">

    <appenders>
        <console name="Console" target="SYSTEM_OUT">
            <patternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
        </console>
    </appenders>

    <loggers>
        <!--  <root level="error">  -->
        <!--  <root level="info">  -->
        <!--  <root level="debug">  -->
        <root level="debug">
            <appenderRef ref="Console"/>
        </root>
    </loggers>

</configuration>